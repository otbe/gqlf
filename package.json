{
  "name": "gqlf",
  "version": "0.0.0",
  "description": "Simple File Explorer exposed via GraphQL",
  "bin": "bin/gqlf.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "test": "tslint src/index.ts",
    "build": "yarn run test && tsc --project tsconfig.json --outDir dist/",
    "preversion": "yarn run test",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "graphql",
    "file",
    "gqlf"
  ],
  "author": "Benjamin Otto",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/otbe/gqlf.git"
  },
  "bugs": {
    "url": "https://github.com/otbe/gqlf/issues"
  },
  "homepage": "https://github.com/otbe/gqlf#readme",
  "devDependencies": {
    "ts-node": "^3.0.4",
    "tslint": "^5.2.0",
    "tslint-eslint-rules": "^4.1.0",
    "typescript": "^2.3.2"
  },
  "dependencies": {
    "@types/argparse": "^1.0.30",
    "@types/express": "^4.0.35",
    "@types/express-graphql": "^0.0.33",
    "@types/graphql": "^0.9.1",
    "@types/graphql-relay": "^0.4.2",
    "@types/node": "^7.0.21",
    "argparse": "^1.0.9",
    "dataloader": "^1.3.0",
    "express": "^4.15.3",
    "express-graphql": "^0.6.5",
    "globby": "^6.1.0",
    "graphql": "^0.9.6",
    "graphql-relay": "^0.5.1",
    "graphql-tools": "^0.11.0"
  }
}
